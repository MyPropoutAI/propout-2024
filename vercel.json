{
  "routes": [
    { "src": "/auth/(.*)", "dest": "/" },
    { "src": "/home/(.*)", "dest": "/" },
    { "src": "/dashboard/(.*)", "dest": "/" },
    { "src": "/(.*)", "dest": "/" },
    {
      "src": "/(.*).js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}